  <main class="main-content position-relative border-radius-lg ">
    <!-- Navbar -->
   <%- include('admin/includes/navbar')%>
    <!-- End Navbar --> 
    <div class="container-fluid py-4">
      <div class="row">
        <div class="col-12">
          <div class="card mb-4">
            <div class="row">

           <div  class="col-10">
            <div class="card-header pb-0">
                <h6>User Table 
                  </h6>
               
              </div>
            </div>
            <div class="col-2">
            <div class="card-header pb-0">
            <h6>
            <a href="/add_user" class="text-secondary font-weight-bold text-xs" data-toggle="tooltip" data-original-title="Edit user">
                <button  type="submit" class="btn btn-sm btn-dark float-right mb-0 d-none d-lg-block">Add User</button>
              </a>
                <!-- <a href="/add_category" class="btn btn-sm btn-dark float-right mb-0 d-block d-lg-none">
                    <button  type="submit" class="btn btn-sm btn-dark float-right mb-0 d-none d-lg-block">Add Category</button>
                </a> -->
               </h6>
            </div>
            </div>
        </div>
            <div class="card-body px-0 pt-0 pb-2">
              <div class="table-responsive p-0">
                <table class="table align-items-center mb-0">
                  <thead>
                    <tr>
                      <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">User</th>
                      <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 ps-2">Mobile</th>
                      <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Status</th>
                      <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Buyer</th>
                      <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Seller</th>
                      <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Reach Buyer</th>
                      <th class="text-secondary opacity-7"></th>
                    </tr>
                  </thead>
                  <tbody>
                    <% if(data.length){
                for(var i = (data.length-1); i >= 0; i--) {%>  
                    <tr>
                      <td>
                        <div class="d-flex px-2 py-1">
                          <% if(data[i].image.length){ %>
                            <div>
                              <!-- <video id="videoPlayer" controls>
                                <source src="<%= data[i].image %>" type="video/mp4">
                              </video> -->
                              <img src="<%= data[i].image %>" class="avatar-sm">
                          
                              &nbsp;
                            </div>
                          <%} else {%>
                           <div>
                            <p class="text-xs text-secondary mb-0">No Image</p>
                            &nbsp;
                           </div>
                            <%}%>
                          <div class="d-flex flex-column justify-content-center">
                            <h6 class="mb-0 text-sm"><%= data[i].user_name %></h6>
                            <p class="text-xs text-secondary mb-0"><%= data[i].email %></p>
                          </div>
                        </div>
                      </td>
                      <td>
                        <p class="text-xs font-weight-bold mb-0">India</p>
                        <p class="text-xs text-secondary mb-0"><%= data[i].mobile %></p>
                      </td>
                      <td class="align-middle text-center text-sm">
                        <% if(data[i].status === true ){ %>
                        <span class="header font-weight-bold text-success">Active</span>
                        <% }else{ %>
                          <span class="header font-weight-bold text-dark">Not Active</span>
                          <% } %>                          
                      </td>
                      <td class="align-middle text-center">
                        <% if(data[i].buy_sell === true ){ %>
                          <a href="#" class="text-secondary font-weight-bold text-xs" data-toggle="tooltip" data-original-title="Edit user">
                            Yes
                          </a>
                        <% }else{ %>
                          <a href="#" class="text-secondary font-weight-bold text-xs" data-toggle="tooltip" data-original-title="Edit user">
                            No
                          </a>
                        <% } %>

                      </td>
                      <td class="align-middle text-center">
                        <% if(data[i].buy_sell === true ){ %>
                          <a href="/user_edit/<%= data[i]._id %>" class="text-secondary font-weight-bold text-xs" data-toggle="tooltip" data-original-title="Edit user">
                            No
                          </a>
                        <% }else{ %>
                          <a href="/user_edit/<%= data[i]._id %>" class="text-secondary font-weight-bold text-xs" data-toggle="tooltip" data-original-title="Edit user">
                            Yes
                          </a>
                        <% } %>

                      </td>
                      <td class="align-middle text-center">
                        <% if(data[i].toThisNoReach === true ){ %>
                        <span class="text-secondary text-xs font-weight-bold">Yes</span>
                        <% }else{ %>
                          <span class="text-secondary text-xs font-weight-bold">No</span>
                          <% }%>
                      </td>
    
                      <td class="align-middle">
                       
                        <a href="/view_user_detail/<%= data[i]._id %>" class="btn btn-primary btn-sm" >View</a>
                         
                        <% if(data[i].status === true ){ %>
                          <button class="btn btn-danger btn-sm" value="<%= data[i]._id %>" onclick="active(this.value)">Deactive</button>
                        <% }else{ %>
                          <button class="btn btn-success btn-sm" value="<%= data[i]._id %>" onclick="active(this.value)">Activate</button>
                         
                        <% } %>


                      </td>
                    </tr>
                    <% }
                        
                      }else{ %>
                          <tr>
                             <td colspan="3">No user</td>
                          </tr>
                       <% } %> 
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
     
      <%- include('admin/includes/footer')%>
    </div>
  </main>
  


    <!--   Core JS Files   -->
  
  <script>
    var win = navigator.platform.indexOf('Win') > -1;
    if (win && document.querySelector('#sidenav-scrollbar')) {
      var options = {
        damping: '0.5'
      }
      Scrollbar.init(document.querySelector('#sidenav-scrollbar'), options);
    }

    function active(id){
    
    location.href='/active_deactive/'+ id;
   
    }

    const VP = document.getElementById('videoPlayer') // player
const VPToggle = document.getElementById('toggleButton') // button
VPToggle.addEventListener('click', function() {
  if (VP.paused) VP.play()
  else VP.pause()
})
    
  </script>
  <!-- Github buttons -->
  <script async defer src="https://buttons.github.io/buttons.js"></script>
  <!-- Control Center for Soft Dashboard: parallax effects, scripts for the example pages etc -->
 
</body>
