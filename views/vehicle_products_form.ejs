                          <input class="form-control" type="text" name="category" placeholder="Vehicle" disabled >
                        </div>
                      </div>
                      <div class="col-md-2">
                        <div class="form-group">
                          <label for="example-text-input" class="form-control-label">Select Subcategory</label>
                        
                          <select class="form-control" name="cars" id="cars">
                            
                            <% if(data.length){
                              for(var i = 0; i< data.length; i++) {%>  
                        
                          <option class="border-radius-lg list-group-item" name="subcategory" value="<%= data[i].sub_category_name %>"><%= data[i].sub_category_name %></option>
                          <% } %> 
                          <% } %>                            
                          </select>
                        </div>
                      </div>
